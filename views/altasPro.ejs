<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title%>
  </title>
  <meta http-equiv="Expires" content="0">
  <meta http-equiv="Last-Modified" content="0">
  <meta http-equiv="Cache-Control" content="no-cache, mustrevalidate">
  <meta http-equiv="Pragma" content="no-cache">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <link
    href="https://fonts.googleapis.com/css2?family=Lato:wght@300;400;700;900&family=Noto+Sans+JP:wght@300;400;700;900&display=swap"
    rel="stylesheet">
  <link rel='stylesheet' href='/stylesheets/general.css' />

  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
  <script src="/socket.io/socket.io.js" type="text/javascript"></script>
  <link rel="stylesheet" type="text/css" href="/stylesheets/normalize.css">
</head>

<body>
  <header class="header">
    <div class="logo">
        <img src="/images/logoC.png" alt="Logo de la empresa">
    </div>
    <nav class="Dos_Column">
      <ul class="nav-links" id="Linksnav">
          <!-- <li><a href="/users/consulEqp">Equipos</a></li> -->
          <li><a href="/users/index">Inicio</a></li>
          <li><a href="/users/consulPro">Ver Productos</a></li>
          <!-- <li><a href="/users/consulMob">Mobiliario</a></li> -->
      </ul>
  </nav>
    <a onclick="CerrarSesion()" class="btn"><button>Cerrar sesión</button></a>
    <a class="btn" href="#"><button><img src="/images/moon.png" width="20px" height="20px"></button></a>
    <a onclick="openNav()" class="menu" href="#"><button>Menu</button></a>
    <div id="mobile-menu" class="overlay">
        <a onclick="closeNav()" href="" class="close">&times;</a>
        <div class="overlay-content" id="MenuCel">
            <!-- <a href="/users/consulEqp">Equipos</a> -->
            <a href="/users/consulPro"> Ver Productos</a>
            <a href="/users/index">Inicio</a>
            <!-- <a href="/users/consulMob">Mobiliario</a> -->
            <a onclick="CerrarSesion()">Cerrar Sesión</a>
            <a href="#">Tema</a>
        </div>
    </div>
</header>
  <script type="text/javascript" src="/javascripts/navbar.js"></script>
  <script type="text/javascript" src="/javascripts/ValidarSesion.js"></script>
  <main class="Contenedor Contenido-centrado Seccion">
    <div class="Contenedor Contenido-centrado Alta">
      <div class="Mini-Header">
        <h4>Añadiendo productos</h4>
      </div>
      <form name="AltaProductos" id="AltaProductos">
        <div class="Cuatro_Column">
          <div>
            <label>FECHA DE INGRESO</label>
            <input autocomplete="off" type="date" id="FecActu" name="FecActu" disabled>
          </div>
          <div>
            <label>Código de barras</label>
            <input autocomplete="off" type="text" id="Cod_Barras" min="1" name="Cod_Barras" required maxlength="45" oninput="mayus(this)" onkeypress="return checkA(event)">
          </div>
          <div>
            <label>Categoría</label>
            <select id="Categoria" name="Categoria" required>
              <option value=""></option>
              <option>PAPELERÍA</option>
              <option>LIMPIEZA</option>
              <option>FERRETERÍA</option>
            </select>
          </div>
          <div>
            <label>Nombre del artículo</label>
            <input autocomplete="off" type="text" id="NomP" name="NomP" required maxlength="45" oninput="mayus(this);" onkeypress="return check(event)">
          </div>
        </div>
        <div class="Cuatro_Column">
          <div>
            <label>Marca del artículo</label>
            <input autocomplete="off" type="text" id="MarcActi" name="MarcActi" required maxlength="100" oninput="mayus(this);" onkeypress="return check(event)">
          </div>
          <div>
            <label>Descripción</label>
            <input autocomplete="off" type="text" id="DescripcionP" name="DescripcionP" required maxlength="400" oninput="mayus(this);" onkeypress="return checkA(event)">
          </div>
          <div>
            <label>Proveedor</label>
            <input autocomplete="off" type="text" id="Proveedor" name="Proveedor" required maxlength="45" oninput="mayus(this);" onkeypress="return check(event)" >
          </div>
          <div>
            <label>Unidad</label>
            <select id="UnidadP" name="UnidadP" required>
              <option value=""></option>
              <option>UNIDAD</option>
              <option>LITROS</option>
              <option>KILOS</option>
              <option>METROS</option>
            </select>
          </div>

        </div>
        <div class="Cuatro_Column">

          <div>
            <label>Cantidad</label>
            <input autocomplete="off" type="number" id="CantidadP" name="CantidadP" required min="1" onkeypress="return checkN(event)">
          </div>
          <div>
            <label>Número de Factura</label>
            <input autocomplete="off" type="text" id="NumFact" name="NumFact" required maxlength="10" oninput="mayus(this);" onkeypress="return checkA(event)">
          </div>
          <div>
            <label>Fecha de Factura</label>
            <input autocomplete="off" type="date" id="FecFact" name="FecFact" required>
          </div>
        </div>
        <div class="Cuatro_Column">
          <input type="submit" style="height: 6rem; width: 15rem;" value="Aceptar" class="BotonAA">
          <a class="BotonReg" href="/users/index" style="height: 6rem; width: 15rem;">Regresar</a>
        </div>
      </form>
    </div>
  </main>
</body>
<script src="/javascripts/FechaActual.js" type="text/javascript"></script>
<script src="/javascripts/SocketsC.js" type="text/javascript"></script>
<script type="text/javascript" src="/javascripts/Mayusculas.js"></script>
<script type="text/javascript" src="/javascripts/NavEspecializado.js"></script>
</html>