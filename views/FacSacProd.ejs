<link rel='stylesheet' href='/stylesheets/modal2.css' />

<div class="card" id="productos">
    <div class="card-header">
        <h4>Registro de productos sacados</h4>
    </div>
    <div class="Dos_Columnas">
        <span>
            <center>
                <h1 id="Adverticement"></h1>
            </center>
        </span>
    </div>
    <div class="Tres_Column">
        <div>
            <label id="start-date">Fecha de inicio:</label>
            <input type="date" id="fechaInicio">
        </div>
        <div>
            <label id="end-date">Fecha de fin:</label>
            <input type="date" id="fechaFin">
        </div>
        <div>
            <label><br></label>
            <button class="BotonFiltro" onclick="FiltrarFechas()">Filtrar</button>
        </div>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table id="DatosProSac">
                <thead>
                    <tr id="firstrow">
                        <th>Código de barras</th>
                        <th>Artículo</th>
                        <th>En existencia</th>
                        <th>Encargado</th>
                        <th>Cantidad de salida</th>
                        <th>Fecha de salida</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
    <input type="button" class="BotonExcel" id="ExcelDownload" onclick="ExcelFacSac()" value="Descargar Excel">
</div>
<script src="/javascripts/Modal2.js" type="text/javascript"></script>