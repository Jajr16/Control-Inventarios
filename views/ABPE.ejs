<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title%>
    </title>
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Last-Modified" content="0">
    <meta http-equiv="Cache-Control" content="no-cache, mustrevalidate">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link
        href="https://fonts.googleapis.com/css2?family=Lato:wght@300;400;700;900&family=Noto+Sans+JP:wght@300;400;700;900&display=swap"
        rel="stylesheet">
    <link rel='stylesheet' href='/stylesheets/general.css' />
    <link rel='stylesheet' href='/stylesheets/consultas.css' />
    <link rel='stylesheet' href='/stylesheets/modal.css' />
    <link rel='stylesheet' href='/stylesheets/modal2.css' />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="/socket.io/socket.io.js" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="/stylesheets/normalize.css">
</head>

<body>
    <header class="header">
        <div class="logo">
            <img src="/images/logoC.png" alt="Logo de la empresa">
        </div>
        <nav class="Dos_Column">
            <ul class="nav-links" id="Linksnav">
                <!-- <li><a href="/users/consulEqp">Equipos</a></li> -->
                <li><a href="/users/index">Inicio</a></li>
                <li><a href="/users/consulPro">Ver Productos</a></li>
                <!-- <li><a href="/users/consulMob">Mobiliario</a></li> -->
            </ul>
        </nav>
        <a onclick="CerrarSesion()" class="btn"><button>Cerrar sesión</button></a>
        <a class="btn" href="#"><button><img src="/images/moon.png" width="20px" height="20px"></button></a>
        <a onclick="openNav()" class="menu" href="#"><button>Menu</button></a>
        <div id="mobile-menu" class="overlay">
            <a onclick="closeNav()" href="" class="close">&times;</a>
            <div class="overlay-content" id="MenuCel">
                <!-- <a href="/users/consulEqp">Equipos</a> -->
                <a href="/users/consulPro"> Ver Productos</a>
                <a href="/users/index">Inicio</a>
                <!-- <a href="/users/consulMob">Mobiliario</a> -->
                <a onclick="CerrarSesion()">Cerrar Sesión</a>
                <a href="#">Tema</a>
            </div>
        </div>
    </header>
    <script type="text/javascript" src="/javascripts/navbar.js"></script>
    <script type="text/javascript" src="/javascripts/ValidarSesion.js"></script>
    <div class="card" id="productos">
        <div class="card-header">
            <h4>Productos en almacén existentes</h4>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <input type="text" id="buscar" onkeyup="buscar()" placeholder="Buscar en tabla"
                    title="Empieza a escribir para buscar">
                <table id="DatosProd">
                    <thead>
                        <tr id="firstrow">
                            <th>Código de barras</th>
                            <th>Categoría</th>
                            <th>Nombre del artículo</th>
                            <th>Marca del artículo</th>
                            <th>Descripción</th>
                            <th>Unidad</th>
                            <th>En Existencia</th>
                            <th>Modificar</th>
                            <th>Eliminar</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="pen">
        <div class="controls">
        </div>
        <div class="overlay cancel-modal"></div>
        <div class="modal-container cancel-modal">
            <div class="Contenedor Contenido-centrado Alta">
                <form name="AltaExist" id="AltaExist">
                    <div class="Cuatro_Column">
                        <div>
                            <label>Fecha de Ingreso</label>
                            <input autocomplete="off" type="date" id="FecActu" name="FecActu" required>
                        </div>
                        <div>
                            <label>Cantidad</label>
                            <input autocomplete="off" type="number" id="CantidadPM" name="CantidadPM" required min="0">
                        </div>
                        <div>
                            <label>Proveedor</label>
                            <input autocomplete="off" type="text" id="ProveedorM" name="ProveedorM" required maxlength="45">
                        </div>
                        <div>
                            <label>Número de Factura</label>
                            <input autocomplete="off" type="text" id="NumFactM" name="NumFactM" required maxlength="10">
                        </div>
                        <div>
                            <label>Fecha de Factura</label>
                            <input autocomplete="off" type="date" id="FecFact" name="FecFact" required>
                        </div>
                    </div>
                    <div class="Cuatro_Column">
                        <input type="submit" id="Modify" class="BotonAE" value="Aceptar">
                        <input type="button" class="BotonReg" value="Cancelar">
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div class="Contenedor_Modal ContMod1">
        <div class="modal modal1-close mod1 modC1">
            <div class="header-modal">
                <h5 id="TituloEliminar" style="padding-bottom: 1rem;"></h5>
                <p class="close title_modal" onclick="Cerrar1()">X</p>
            </div>
            <div style="width: 80%;">
                <form id="BajaExist" name="BajaExist">
                    <div class="Dos_Column">
                        <input autocomplete="off" type="number" id="CantidadP" name="CantidadP" required min="1">
                    </div>
                    <div class="Dos_Columnas">
                        <input type="submit" class="BotonAE">
                        <input type="button" class="BotonCancelar" onclick="Cerrar1()" value="Cancelar">
                    </div>
                </form>
            </div>
        </div>
    </div>

</body>
<script src="/javascripts/SocketsC.js" type="text/javascript"></script>
<script src='https://s3.amazonaws.com/popmotion.io/static/dist/popmotion.min.js'></script>
<script src="/javascripts/modalAltProd.js" type="text/javascript"></script>
<script src="/javascripts/Modal2.js" type="text/javascript"></script>
<script src="/javascripts/FechaActual.js" type="text/javascript"></script>

</html>