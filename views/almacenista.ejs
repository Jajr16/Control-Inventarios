<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/css/select2.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/js/select2.min.js"></script>
<script></script>
<main>
    <div class="card" id="productos">
        <div class="card-header">
            <h4>Productos a Entregar</h4>
        </div>
        <div class="Dos_Columnas">
            <span>
                <center>
                    <h1 id="Adverticement"></h1>
                </center>
            </span>
        </div>
        <div class="Tres_Column">
            <div>
                <label id="start-date">Fecha de inicio:</label>
                <input type="date" id="fechaInicio">
            </div>
            <div>
                <label id="end-date">Fecha de fin:</label>
                <input type="date" id="fechaFin">
            </div>
            <div>
                <label><br></label>
                <button class="BotonFiltro" onclick="FiltrarFechas()">Filtrar</button>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table id="Requests">
                    <thead>
                        <tr id="firstrow">
                            <th>Fecha</th>
                            <th>Código de barras</th>
                            <th>Nombre del artículo</th>
                            <th>Marca del artículo</th>
                            <th>Cantidad</th>
                            <th>Solicitante</th>
                            <th>Estatus de entrega</th>
                            <th>Recibido por solicitante</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
        <input type="button" class="BotonExcel" id="ExcelDownload" onclick="Excel('ExcelSoli')" value="Descargar Excel">
    </div>
</main>
<script src="/javascripts/FechaActual.js" type="text/javascript"></script>